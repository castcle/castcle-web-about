  "bugs": {
    "url": "https://github.com/castcle/castcle-web-about/issues"
  },
  "homepage": "https://github.com/castcle/castcle-web-about#readme",
  "devDependencies": {
    "grunt": "^1.4.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0"
  }
}
